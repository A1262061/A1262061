@model IEnumerable<ShoppingCartMVC.Models.ProductStyle>



<select class="form-control" name="P_Style">
    @if (Model.Count() == 0)
    {

        <option value="一般">一般</option>

    }
    else
    {
        foreach (var item in Model)
        {

            <option value="@item.PStyle_Name">@item.PStyle_Name</option>
        }
    }


</select>


















