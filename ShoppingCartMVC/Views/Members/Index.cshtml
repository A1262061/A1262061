@model IEnumerable<ShoppingCartMVC.Models.Member>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.M_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.M_Account)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.M_Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.M_Tel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.M_Gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.M_Address)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.M_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.M_Account)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.M_Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.M_Tel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.M_Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.M_Address)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.M_num }) |
                @Html.ActionLink("Details", "Details", new { id = item.M_num }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.M_num })
            </td>
        </tr>
    }

</table>
