@model IEnumerable<ShoppingCartMVC.Models.Orders>

@{
    ViewBag.Title = "OrderList";
}


<p>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.O_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.O_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.O_Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.O_Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Member.M_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.O_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.O_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.O_Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.O_Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member.M_Name)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.O_num }) |
        </td>
    </tr>
}

</table>
